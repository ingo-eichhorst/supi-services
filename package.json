{
  "name": "backend",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "docker-dev": "COMPOSE_FILE=docker-compose.dev.yml docker-compose up --build",
    "docker-dev-all": "COMPOSE_FILE=docker-compose.yml:docker-compose.dev.yml docker-compose up --build",
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^4.18.2",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "moleculer-repl": "^0.4.0"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.5",
    "moleculer": "^0.12.0",
    "moleculer-web": "^0.6.0",
    "nats": "^0.8.4",
    "osx-temperature-sensor": "^1.0.1",
    "serialport": "^6.1.1",
    "speedtest-net": "^1.4.2",
    "ssh2": "^0.6.0",
    "systeminformation": "^3.37.8"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
